jQuery(document).ready(function() {

	var links = {
		'fb' : jQuery('#edit-brown-social-media-url-fb').val(),
		'twitter' : jQuery('#edit-brown-social-media-url-twitter').val(),
		'youtube' : jQuery('#edit-brown-social-media-url-youtube').val(),
		'googleplus' : jQuery('#edit-brown-social-media-url-googleplus').val(),
		'instagram' : jQuery('#edit-brown-social-media-url-instagram').val(),
		'flickr' : jQuery('#edit-brown-social-media-url-flickr').val(),
		'linkedin' : jQuery('#edit-brown-social-media-url-linkedin').val(),
		'vimeo' : jQuery('#edit-brown-social-media-url-vimeo').val(),
		'email' : jQuery('#edit-brown-social-media-url-email').val(),
		// Add other links here
		
		};
	var module = "brown-social-media";	

	jQuery.each(links, function(i, value) { 
		if(value === ""){
			jQuery('#edit-brown-social-media-url-'+i+'-wrapper').hide();
			jQuery("#edit-brown-social-media-check-"+i).attr('unchecked',false);		
		}
		else{
			jQuery('#edit-brown-social-media-url-'+i+'-wrapper').show();
			jQuery("#edit-brown-social-media-check-"+i).attr('checked','checked');
		}
	  
	});
	
	jQuery(".form-checkbox").change(function(){
		var str=this.id;
		var n=str.match(/fb|twitter|youtube|googleplus|instagram|flickr|linkedin|vimeo|email/g); // Add more key words here
		if(jQuery(this).is(":checked")){			
			jQuery('#edit-brown-social-media-url-'+n+'-wrapper').show();	
			jQuery('#edit-brown-social-media-url-'+n).focus();
		}
		else{
			jQuery('#edit-brown-social-media-url-'+n+'-wrapper').hide();
		}
	});
	jQuery(".form-item").css("background-color","transparent");
	jQuery(".form-item").css("border","none");
	jQuery(".form-item").css("webkit-box-shadow","none");
});



}
